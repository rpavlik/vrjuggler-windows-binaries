<?xml version="1.0" encoding="UTF-8"?>
<?org-vrjuggler-jccl-settings definition.version="3.1"?>
<definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.vrjuggler.org/jccl/xsd/3.1/definition" name="pinch_glove" icon_path="jar:file:${VJ_DATA_DIR}/beans/customeditors/PinchGloveEditor.jar!/org/vrjuggler/vrjconfig/customeditors/pinchglove/images/PinchGloveIcon.png" xsi:schemaLocation="http://www.vrjuggler.org/jccl/xsd/3.1/definition http://www.vrjuggler.org/jccl/xsd/3.1/definition.xsd">
   <definition_version version="1" label="Pinch Glove">
      <abstract>false</abstract>
      <help>Configuration for the Fakespace Pinch Glove. (&lt;a href="http://vrjuggler.org/docs/vrjuggler/3.0/configuration.guide/configuring_vr_juggler/apbs02.html"&gt;more on Pinch Gloves&lt;/a&gt;, &lt;a href="http://www.infiscape.com/documentation/vrjuggler-config/3.0/configuring_vr_juggler/ch02.html"&gt;more on input devices&lt;/a&gt;)</help>
      <parent>glove_device</parent>
      <parent>digital_device</parent>
      <category>/Devices/Glove</category>
      <property valuetype="string" variable="false" name="port">
         <help>The name of the serial port to which the Pinch Glove device is connected. The name to use will be based on platform-specific conventions such as COM1, /dev/ttyS0, /dev/ttyd2, etc. (&lt;a href="http://vrjuggler.org/docs/vrjuggler/3.0/configuration.guide/configuring_vr_juggler/apbs02.html"&gt;more ...&lt;/a&gt;)</help>
         <value label="Port" defaultvalue="/dev/ttyd1"/>
      </property>
      <property valuetype="integer" variable="false" name="baud">
         <help>Serial port speed. (&lt;a href="http://vrjuggler.org/docs/vrjuggler/3.0/configuration.guide/configuring_vr_juggler/apbs02.html"&gt;more ...&lt;/a&gt;)</help>
         <value label="Baud Rate" defaultvalue="9600"/>
      </property>
      <property valuetype="configelementpointer" variable="false" name="glove_position">
         <help>The positional device that tracks the glove position. That is, the tracker on the glove. (&lt;a href="http://vrjuggler.org/docs/vrjuggler/3.0/configuration.guide/configuring_vr_juggler/apbs02.html"&gt;more ...&lt;/a&gt;)</help>
         <value label="Glove Position"/>
         <allowed_type>alias</allowed_type>
         <allowed_type>position_proxy</allowed_type>
      </property>
      <upgrade_transform/>
   </definition_version>
</definition>

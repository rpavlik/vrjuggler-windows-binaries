        <ID NAME="VID_DoNotWant" VAL="4"/>
        <ID NAME="VID_GetOutOfWay" VAL="5"/>
    </DEFINE>
    <RULE ID="VID_Hello" TOPLEVEL="ACTIVE">
        <P>
            <L>
                <P>Hello</P>
            </L>
        </P>
    </RULE>
    <RULE ID="VID_Goodbye" TOPLEVEL="ACTIVE">
        <P>
            <L>
                <P>Goodbye</P>
            </L>
        </P>
    </RULE>
    <RULE ID="VID_Buy" TOPLEVEL="ACTIVE">
        <P>
            <L>
